    <views default="office">
        <perspective id="office" near="0.1" far="500" angle="45">
            <from x="10" y="10" z="10" />
            <to x="0" y="6" z="0" />
        </perspective>
        <perspective id="door" near="0.1" far="500" angle="45">
            <from x="-10" y="10" z="-5" />
            <to x="0" y="6" z="0" />
