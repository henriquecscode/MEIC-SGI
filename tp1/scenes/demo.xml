        <ortho id="top" near="0.1" far="500" left="-20" right="20" bottom="-20" top="20">
            <from x="0" y="10" z="0" />
            <to x="0" y="0" z="0" />
            <up x="1" y="0" z="0" />
        </ortho>

        <ortho id="frame" near="0.1" far="500" left="-20" right="20" bottom="-20" top="20">
            <from x="10" y="5" z="5" />
            <to x="0" y="5" z="5" />
            <up x="0" y="1" z="0" />
        </ortho>

        <perspective id="desk" near="0.1" far="500" angle="45">
            <from x="10" y="8" z="3" />
            <to x="0" y="8" z="3" />
        </perspective>
